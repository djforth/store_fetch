{
  "browser": true,
  "collectCoverageFrom": [
    "<rootDir>/src/**/*.{js,jsx}"
  ],
  "coveragePathIgnorePatterns": [
    "<rootDir>/node_modules/",
    "<rootDir>/__tests__/",
    "<rootDir>/__tests__/__helpers__"
  ],
  "roots": [
    "<rootDir>/__tests__/",
    "<rootDir>/src/"
  ],
  "moduleNameMapper": {},
  "modulePaths": [
    "<rootDir>/src/"
  ],
  "modulePathIgnorePatterns": [
    "<rootDir>/lib/",
    "<rootDir>/node_modules/"
  ],
  "setupFilesAfterEnv": [
    "<rootDir>/__tests__/setupTests.js"
  ],
  "testPathIgnorePatterns": [
    "<rootDir>/__tests__/setupTests.js",
    "<rootDir>/__tests__/__helpers__",
    "<rootDir>/__tests__/__dummy__",
    "<rootDir>/lib",
    "<rootDir>/node_modules",
    "<rootDir>/__mocks__"
  ],
  "verbose": true
}